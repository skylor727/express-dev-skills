<%- include('../partials/header') %>
    <ul>
        <% skills.forEach(function(s) { %>

            <li>
                <form action="/skills/<%= s.id %>?_method=DELETE" class="delete-form" method="POST"><button
                        type="submit">X</button></form>

                <a href="/skills/<%= s.id %>">

                    <%= s.skill %>
                </a>
                - Mastered -
                <%= s.mastered ? 'Yes' : 'No' %>
            </li>
            <% }); %>
    </ul>
    <%- include('../partials/footer') %>